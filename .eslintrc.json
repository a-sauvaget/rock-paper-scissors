{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": 0,
        "no-plusplus": "off",
        "no-tabs": "off",
        "prefer-template":"off",
        "indent":"off"
    }

}
